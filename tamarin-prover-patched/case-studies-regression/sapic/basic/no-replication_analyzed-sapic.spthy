theory NoReplication begin

// Function signature and definition of the equational theory E

functions: check_rep/2, fst/1, get_rep/1, pair/2, rep/2 [private], snd/1
equations:
    check_rep(rep(m, loc), loc) = m,
    fst(<x.1, x.2>) = x.1,
    get_rep(rep(m, loc)) = m,
    snd(<x.1, x.2>) = x.2

heuristic: p

rule (modulo E) Init_:
   [ ] --[ Init( ) ]-> [ State_( ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) new_s_:
   [ State_( ), Fr( s ) ] --> [ State_1( s ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) event_Secrets_1:
   [ State_1( s ) ] --[ Event( ), Secret( s ) ]-> [ State_11( s ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) out_s_11:
   [ State_11( s ) ] --> [ State_111( s ), Out( s ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) Zero_111:
   [ State_111( s ) ] --> [ ]

  /* has exactly the trivial AC variant */

restriction single_session:
  "∀ #i #j. ((Init( ) @ #i) ∧ (Init( ) @ #j)) ⇒ (#i = #j)"
  // safety formula

lemma onlyOneSecret:
  all-traces
  "∀ #i #j x y. ((Secret( x ) @ #i) ∧ (Secret( y ) @ #j)) ⇒ (x = y)"
/*
guarded formula characterizing all counter-examples:
"∃ #i #j x y. (Secret( x ) @ #i) ∧ (Secret( y ) @ #j) ∧ ¬(x = y)"
*/
simplify
solve( State_1( x ) ▶₀ #i )
  case new_s_
  solve( State_1( y ) ▶₀ #j )
    case new_s_
    by contradiction /* from formulas */
  qed
qed

/* All well-formedness checks were successful. */

end
